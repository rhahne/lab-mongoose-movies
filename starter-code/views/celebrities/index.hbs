<a href="/celebrities/new">Create a celebrity</a>

{{#each celebritiesData}}
    <h2>{{ name }}</h2>
    <a href="/celebrities/{{ id }}">Details</a>
    <form method="POST" action="/celebrities/{{ id }}/delete">
        <button type="submit">Delete</button>
    </form>
    <a href="/celebrities/{{ id }}/edit">Edit</a>
{{/each}}

